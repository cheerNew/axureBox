<view class="uni-tab-bar" bind:__l="__l"><scroll-view class="uni-swiper-tab" id="tab-bar" scroll-x scroll-left="{{scrollLeft}}"><block wx:for="{{tabBars}}" wx:for-item="tab" wx:for-index="index" wx:key="id"><view class="{{['swiper-tab-list',tabIndex==index?'active':'']}}"><image src="{{tab.imgurl}}"></image><view data-current="{{index}}" data-id="{{tab.id}}" data-event-opts="{{[['tap',[['tapTab',['$event']]]]]}}" bindtap="__e">{{tab.name}}</view></view></block></scroll-view><view class="swiper-box"><block wx:for="{{newsitems}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="mainBox"><view class="title-box"><image src="{{item.titleIcon}}"></image><text>{{item.title}}</text></view><view class="banner-main"><image src="{{item.bannerUrl}}"></image><text class="remen">{{item.bannerTitle}}</text></view><view class="title-main"><text class="main-left">{{item.content}}</text><text class="main-right">{{"￥"+item.value+"元起"}}</text></view><view class="scond-title">{{item.smallTitle}}</view><view class="main-bottom"><view class="bottom-left"><image src="../../static/xin.png"></image><text>{{item.xinValue}}</text><image class="imgstyle" src="../../static/yanjing.png"></image><text>{{item.jinValue}}</text></view><view class="bottom-right"><image src="../../static/xiaoxi.png"></image><text>{{item.commentValue}}</text><image src="../../static/right.png"></image></view></view></view></block></view></view>